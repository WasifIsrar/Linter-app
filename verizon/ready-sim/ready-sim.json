    "/m2m/v2/triggers": {
      "get": {
        "tags": [
          "Retrieve the Triggers"
        ],
        "summary": "Retrieve all triggers.",
        "description": "Retrieves all of the available triggers for pseudo-MDN.",
        "operationId": "GetAllAvailableTriggers",
