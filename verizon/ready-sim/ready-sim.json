              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TriggerValueResponse"
                },
                "example": {
                  "triggers": [
                    {
                      "triggerId": "2874DEC7-26CF-4797-9C6A-B5A2AC72D526",
                      "triggerName": "PromoAlerts_0000000000-00001_23456789",
                      "accountName": "0000123456-000001",
                      "organizationName": "Optional group name",
                      "triggerCategory": "PromoAlerts",
                      "triggerAttributes": [
                        {
                          "key": "ServicePlan"
                        },
                        {
                          "key": "DataPercentage50",
                          "value": false
                        },
                        {
                          "key": "DataPercentage75",
                          "value": false
                        },
                        {
                          "key": "DataPercentage90",
                          "value": false
                        },
                        {
                          "key": "DataPercentage100",
                          "value": false
                        },
                        {
                          "key": "SmsPercentage50",
                          "value": false
                        },
                        {
                          "key": "SmsPercentage75",
                          "value": false
                        },
                        {
                          "key": "SmsPercentage90",
                          "value": false
                        },
                        {
                          "key": "SmsPercentage100",
                          "value": true
                        },
                        {
                          "key": "NoOfDaysB4PromoExp",
                          "value": 5
                        },
                        {
                          "key": "EnablePromoExp",
                          "value": true
                        }
                      ],
                      "createdAt": "2021-10-21T23:57:003.397Z",
                      "modifiedAt": "2021-10-21T23:57:003.397Z"
                    }
                  ]
                }
              }
            }
          },
          "default": {
            "description": "Error response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ReadySimRestErrorResponse"
                },
                "example": {
                  "errorCode": "The 3-digit HTML error code",
                  "errorMessage": "More details about the error"
                }
              }
            }
          }
        }
      },
